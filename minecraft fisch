        local KingGui = game.CoreGui:FindFirstChild("ScreenGui") -- Replace with the actual name
        if KingGui then
            toggleVisibility(KingGui)
        else
            warn("1")
        end
    end)

    C.CornerRadius = UDim.new(1, 0)
    C.Parent = B
end

